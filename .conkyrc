conky.config={
    out_to_x=false,
    own_window=false,
    out_to_console=true,
    background=false,
    max_text_width=0,
    border_inner_margin=0,
    border_outer_margin=0,
    update_interval=1.0,
    cpu_avg_samples=2,
    use_spacer="left",
    override_utf8_locale=true,
    use_xft=true,
    font="tamsyn:size=8",
}

conky.text = [[
[{ "full_text": "1:${cpu cpu1}% 2:${cpu cpu2}% 3:${cpu cpu3}% 4:${cpu cpu4}%", "color": "\#b16286" },
{ "full_text": ": $memperc%", "color": "\#8ec07c" },
{ "full_text": ":${upspeed ens33}", "color": "\#d65d0e" },
{ "full_text": ":${downspeed ens33}", "color": "\#d65d0e" },
{ "full_text": "${time %a, %d %b %G %H:%M}", "color": "\#928374" }],
]]
